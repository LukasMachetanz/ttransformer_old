    "publish": "npm run publish:angular && npm run publish:core && npm run publish:transformer-support",
    "deploy": "npm run clean && npm run version && npm run clean && npm run build && npm run clean && npm run publish",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
