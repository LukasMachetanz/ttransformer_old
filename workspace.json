{
  "version": 1,
  "projects": {
    "core": {
      "root": "libs/core",
      "sourceRoot": "libs/core/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/core/tsconfig.lib.json",
              "libs/core/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/core/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/core/jest.config.js",
            "tsConfig": "libs/core/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/core",
            "tsConfig": "libs/core/tsconfig.lib.json",
            "packageJson": "libs/core/package.json",
            "main": "libs/core/src/index.ts",
            "assets": ["libs/core/*.md"]
          }
        }
      }
    },
    "angular-karma-demo": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/angular-karma-demo",
      "sourceRoot": "apps/angular-karma-demo/src",
      "prefix": "ttransformer",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/angular-karma-demo",
            "index": "apps/angular-karma-demo/src/index.html",
            "main": "apps/angular-karma-demo/src/main.ts",
            "polyfills": "apps/angular-karma-demo/src/polyfills.ts",
            "tsConfig": "apps/angular-karma-demo/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/angular-karma-demo/src/favicon.ico",
              "apps/angular-karma-demo/src/assets"
            ],
            "styles": ["apps/angular-karma-demo/src/styles.scss"],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/angular-karma-demo/src/environments/environment.ts",
                  "with": "apps/angular-karma-demo/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "angular-karma-demo:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "angular-karma-demo:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "angular-karma-demo:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/angular-karma-demo/tsconfig.app.json",
              "apps/angular-karma-demo/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/angular-karma-demo/**/*"]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/angular-karma-demo/src/test.ts",
            "tsConfig": "apps/angular-karma-demo/tsconfig.spec.json",
            "karmaConfig": "apps/angular-karma-demo/karma.conf.js",
            "polyfills": "apps/angular-karma-demo/src/polyfills.ts",
            "styles": [],
            "scripts": [],
            "assets": []
          }
        }
      }
    },
    "angular-karma-demo-e2e": {
      "root": "apps/angular-karma-demo-e2e",
      "sourceRoot": "apps/angular-karma-demo-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/angular-karma-demo-e2e/cypress.json",
            "tsConfig": "apps/angular-karma-demo-e2e/tsconfig.e2e.json",
            "devServerTarget": "angular-karma-demo:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "angular-karma-demo:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["apps/angular-karma-demo-e2e/tsconfig.e2e.json"],
            "exclude": [
              "**/node_modules/**",
              "!apps/angular-karma-demo-e2e/**/*"
            ]
          }
        }
      }
    },
    "angular": {
      "root": "libs/angular",
      "sourceRoot": "libs/angular/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/angular/tsconfig.lib.json",
              "libs/angular/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!libs/angular/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/angular/jest.config.js",
            "tsConfig": "libs/angular/tsconfig.spec.json",
            "passWithNoTests": true
          }
        },
        "build": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/angular",
            "tsConfig": "libs/angular/tsconfig.lib.json",
            "packageJson": "libs/angular/package.json",
            "main": "libs/angular/src/index.ts",
            "assets": ["libs/angular/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/angular"
  },
  "schematics": {
    "@nrwl/workspace": {
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/cypress": {
      "cypress-project": {
        "linter": "eslint"
      }
    },
    "@nrwl/react": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      },
      "storybook-configuration": {
        "linter": "eslint"
      }
    },
    "@nrwl/next": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/web": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/node": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/nx-plugin": {
      "plugin": {
        "linter": "eslint"
      }
    },
    "@nrwl/nest": {
      "application": {
        "linter": "eslint"
      }
    },
    "@nrwl/express": {
      "application": {
        "linter": "eslint"
      },
      "library": {
        "linter": "eslint"
      }
    },
    "@nrwl/angular:application": {
      "unitTestRunner": "karma",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "karma"
    }
  }
}
